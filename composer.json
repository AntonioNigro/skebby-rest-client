{
    "name": "fazland/skebby-rest-client",
    "description": "Unofficial Skebby Rest Client",
    "type": "library",
    "authors": [
        {
            "name": "Massimiliano Braglia",
            "email": "massimiliano.braglia@fazland.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "ext-curl": "*",
        "giggsey/libphonenumber-for-php": "^7.0",
        "symfony/options-resolver": "^2.7|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8|^5.0",
        "kcs/function-mock": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Fazland\\SkebbyRestClient\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fazland\\SkebbyRestClient\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "license": "MIT"
}
